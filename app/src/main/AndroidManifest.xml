<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asheransari.habittrack">

    <application
        android:allowBackup="true"
        android:icon="@drawable/todo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".login" />
        <activity android:name=".sign_up">
        <meta-data
        android:name="android.support.PARENT_ACTIVITY"
        android:value="com.example.asheransari.habittrack.login"/>
        </activity>
        <activity android:name=".splashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".insert_data" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.asheransari.habittrack.MainActivity"/>
        </activity>
        <activity android:name=".update_activity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.asheransari.habittrack.MainActivity"/>
        </activity>
    </application>

</manifest>